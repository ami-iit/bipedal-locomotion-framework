{
    "names":
    {
        "YARP" :
        {
            "cmake-args": [
                "-DYARP_COMPILE_ALL_FAKE_DEVICES:BOOL=ON",
                "-DYARP_COMPILE_GUIS=OFF"
            ]
        },
        "casadi" :
        {
            "cmake-args": [
                "-DWITH_IPOPT=BOOL:ON",
                "-DWITH_OSQP:BOOL=ON",
                "-DUSE_SYSTEM_WISE_OSQP=BOOL:ON"
            ],
            "dependencies": [
                "osqp"
            ]
        },
        "icub-models":
        {
            "cmake-args": [
                "-DICUB_MODELS_USES_PYTHON:BOOL=ON",
            ]
        },
        "manif":
        {
            "cmake-args": [
                "-DBUILD_PYTHON_BINDINGS:BOOL=ON"
            ]
        },
    }
}